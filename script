const ltvKPIsRangeName = "B6:E22";
const groupingsRangeName = "G6:I21";
const listOfRulesRange = "B3";
const widthOfRulesRange = 7;
const listOfRawData = "I3"
const widthOfRawData = 5;

const nameMainMenu = "Wizard";
