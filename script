// SAMPLE
// *****************************************************************************************************************************
function robot003(spreadsheetID, appID, url)
{
  Logger.log("Robot 3 launched");
  Logger.log("App ID: " + appID + ", Spreadsheet ID:" + spreadsheetID);
  Logger.log("");
  return;
}
